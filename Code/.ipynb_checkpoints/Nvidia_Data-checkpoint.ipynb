    "# Calculate % Yearly return \n",
    "\n",
    "# Create a Year column\n",
    "\n",
    "df['Date'] = pd.to_datetime(df['Date'])\n",
    "df['Year'] = df['Date'].dt.year\n",
    "\n",
    "# Group by year\n",
    "\n",
    "yearly_prices = df.groupby('Year').agg(\n",
    "    Start_Price=('Close', 'first'),\n",
    "    End_Price=('Close', 'last')\n",
    ").reset_index()\n",
    "\n",
    "# Calculate yearly % return\n",
    "\n",
    "yearly_prices['%Return'] = ((yearly_prices['End_Price'] - yearly_prices['Start_Price']) / yearly_prices['Start_Price']) * 100 \n",
    "\n",
    "yearly_prices['Positive_Return'] = yearly_prices['%Return'].apply(lambda x: x if x > 0 else 0)\n",
    "yearly_prices['Negative_Return'] = yearly_prices['%Return'].apply(lambda x: x if x < 0 else 0)\n",
    "\n",
    "print(yearly_prices)\n",
    "# Save to CSV \n",
    "yearly_prices.to_csv(r\"C:\\Users\\ragul\\Downloads\\Nvidia Project\\Datasets\\nvidia_yearly_returns.csv\", index=False)"
   ]
