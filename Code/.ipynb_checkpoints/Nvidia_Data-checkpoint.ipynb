   "source": [
    "# Calculate Annual Percentage return\n",
    "\n",
    "# Create year column\n",
    "\n",
    "nasdaq['Year'] = nasdaq['Date'].dt.year\n",
    "\n",
    "# Group by year\n",
    "\n",
    "yearly_prices = nasdaq.groupby('Year').agg(\n",
    "    Start_Price=('Close', 'first'),\n",
    "    End_Price=('Close', 'last')\n",
    ").reset_index()\n",
    "\n",
    "# Calculate yearly returns \n",
    "\n",
    "yearly_prices['Yearly_Return_%'] = ((yearly_prices['End_Price'] - yearly_prices['Start_Price']) / yearly_prices['Start_Price']) * 100\n",
    "yearly_prices['Positive_Return'] = yearly_prices['Yearly_Return_%'].apply(lambda x: x if x > 0 else 0)\n",
    "yearly_prices['Negative_Return'] = yearly_prices['Yearly_Return_%'].apply(lambda x: x if x < 0 else 0)\n",
    "\n",
    "yearly_prices"
   ]
  },
